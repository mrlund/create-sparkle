{
  "name": "sparkle-starter",
  "version": "0.0.1",
  "description": "Made with Sparkle, Sparkle is the free open-source platform for creating learning content on web, mobile and desktop.",
  "homepage": "https://girlsinclearning.com/",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch\" \"npm run dev\"",
    "dev": "lite-server",
    "watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "local": "concurrently --kill-others \"webpack --watch --mode development --env.local=true\" \"npm run dev\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mrlund/create-sparkle.git"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lite-server": "^2.5.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "license": "MIT",
  "dependencies": {
    "@sparkle-learning/core": "0.0.1"
  },
  "keywords": [
    "course",
    "e-learning",
    "sparkle",
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app"
  ]
}
